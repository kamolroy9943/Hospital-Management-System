@model IEnumerable<HospitalManagement.Data.Doctor>

@{
    ViewBag.Title = "RetiredList";
}

<h2>RetiredList</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DoctorName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.age)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Salary)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Contact)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ContactNo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ContactEmail)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Departments)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Degrees)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SurgeryOrMedicine)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.JoinningDate)
        </th>
        
        <th>
            @Html.DisplayNameFor(model => model.RetireDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Updated)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UpdatedBy)
        </th>
        <th>
          Image
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DoctorName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.age)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Salary)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Contact)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ContactNo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ContactEmail)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Departments)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Degrees)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SurgeryOrMedicine)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.JoinningDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RetireDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Updated)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UpdatedBy)
        </td>
        <td>
            <img src=" @Url.Content(item.ImagePath)" width="50" height="50"
        </td>
        <td>
          
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
           
        </td>
    </tr>
}

</table>

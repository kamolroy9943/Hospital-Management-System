@model IEnumerable<HospitalManagement.Data.Seat>

@{
    ViewBag.Title = "FromWhereList";
}

<h2>FromWhereList</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
       

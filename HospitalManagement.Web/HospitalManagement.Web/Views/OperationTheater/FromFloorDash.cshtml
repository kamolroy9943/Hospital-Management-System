@model IEnumerable<HospitalManagement.Data.OperationTheater>


@{
    ViewBag.Title = "FromFloorDash";
}

<h2>FromFloorDash</h2>

<p>
    @Html.ActionLink("Add New Operation Theater in this Floor", "FromFloorCreate",new {id= @ViewBag.FloorId })
</p>
<table class="table">
    <tr>
       
       
        <th>
            @Html.DisplayNameFor(model => model.Number)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BuildingName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FloorNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OperationDone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Updated)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UpdatedBy)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
       
        <td>
            @Html.DisplayFor(modelItem => item.Number)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BuildingName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FloorNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OperationDone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Updated)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UpdatedBy)
        </td>
        <td>
           
            @Html.ActionLink("Details", "Details", new { id=item.Id, from = ViewBag.from, fromid = ViewBag.fromid }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id, from = ViewBag.from, fromid = ViewBag.fromid })
        </td>
    </tr>
}

</table>
